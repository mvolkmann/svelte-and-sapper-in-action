{
  "name": "travel-packing",
  "version": "1.0.0",
  "license": "MIT",
  "repository": "git@github.com:mvolkmann/svelte-and-sapper-in-action.git",
  "scripts": {
    "build": "rollup -c",
    "build-storybook": "build-storybook",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "dev": "rollup -c -w",
    "format": "prettier --write 'src/**/*.{css,html,js,svelte}'",
    "lint": "eslint --fix --quiet src --ext .js,.svelte",
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "start": "sirv public --single",
    "storybook": "start-storybook -p 6006",
    "test": "jest src",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "dialog-polyfill": "^0.5.0",
    "page": "^1.11.5",
    "sirv-cli": "^0.4.5",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/svelte": "^5.3.18",
    "@testing-library/svelte": "^3.0.0",
    "babel-jest": "^25.2.6",
    "babel-loader": "^8.1.0",
    "cypress": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-svelte3": "^2.7.3",
    "jest": "^25.2.7",
    "prettier": "^1.19.1",
    "prettier-plugin-svelte": "^0.7.0",
    "rollup": "^2.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-livereload": "^1.1.0",
    "rollup-plugin-svelte": "^5.2.1",
    "rollup-plugin-terser": "^5.3.0",
    "svelte": "^3.49.0",
    "svelte-jester": "^1.0.5",
    "svelte-loader": "^2.13.6"
  }
}
